<STYLE TYPE="text/css" media="all">
  .ui-autocomplete {
      position: absolute;
      cursor: default;
      max-height: 180px;
      font-size: 15px;
      border-radius: 5px;
      overflow-y: scroll;
      overflow-x: hidden;}
  </STYLE>

<script>
  $(function(){
$('#searchName').autocomplete({
source: function(req,res){
  $.ajax({
    url:"autocomplete/",
    dataType:"jsonp",
    type:"GET",
    data:req,
    success: function(data){
      res(data)
    },
    error: function(err){
      console.log(err.status);
    }
  });
},
minLength:1,
select: function(event,ui){
  if(ui.item){
    $('#searchName').val(ui.item.label);
  }
}
});
  });

  function validateForm() {
  var x = document.forms["myForm"]["devname"].value;
  if (x == "") {
    alert("Please enter a device name to search!!");
    return false;
  }
}

  </script> 
  
